		for (int i = 1; i <= 50; i++) {
			alumnos alumno=new alumnos("Nombre "+i, "Apellido "+i, (int) (Math.random()*10+1), (int) (Math.random()*10+1), (int) (Math.random()*10+1));
			listaAlumnos.add(alumno);
		}
		Leer.mensaje("Nombre    Apellido    1ยบ Trimestre    2ยบ Trimestre   2 Trimestre");
